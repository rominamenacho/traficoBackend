package com.nuebus;

/**
 *
 * @author Valeria
 */

@SpringBootApplication
@EnableConfigurationProperties
public class TraficoNuebustApplicationWeb extends SpringBootServletInitializer{
    
    
	@Override
        protected SpringApplicationBuilder configure(SpringApplicationBuilder application) {
            return application.sources(TraficoNuebustApplicationWeb.class);
        }

        public static void main(String[] args) throws Exception {
            SpringApplication.run(TraficoNuebustApplicationWeb.class, args);
        }      
        
}
